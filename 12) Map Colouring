def mapcoloring(graph, colors):
    colored = {}
    for node in sorted(graph, key=lambda x: len(graph[x]), reverse=True):
        used_colors = {colored[neighbor] for neighbor in graph[node] if neighbor in colored}
        available_colors = colors - used_colors
        colored[node] = next(iter(available_colors)) if available_colors else None
    return colored
graph = {
    0: [1, 2],
    1: [0, 2],
    2: [0, 1, 3],
    3: [0, 2],
    4: [1,3]  }
colors = {'Red', 'Green', 'Blue'}
coloring = mapcoloring(graph, colors)
print("Map Coloring Result:", coloring)
